{
  "name": "finainexus-backend",
  "version": "1.0.0",
  "description": "FinNexusAI Backend API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run lint && npm run test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "node scripts/migrate.js",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "seed": "node scripts/seed.js",
    "docker:build": "docker build -t finainexus/backend:latest .",
    "docker:run": "docker run -p 3000:3000 finainexus/backend:latest",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "health": "curl http://localhost:3000/api/v1/health"
  },
