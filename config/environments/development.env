# FinNexusAI Development Environment Configuration
# Local development settings

# Application
NODE_ENV=development
PORT=3000
API_VERSION=v1

# Database - Local PostgreSQL
DATABASE_URL=postgresql://postgres:password@localhost:5432/finnexusai_dev
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10
DATABASE_SSL=false

# Redis - Local Redis
REDIS_URL=redis://localhost:6379/0

# JWT Configuration
JWT_SECRET=dev-jwt-secret-key-not-for-production
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=dev-refresh-secret-key-not-for-production
JWT_REFRESH_EXPIRES_IN=7d

# External APIs - Development Keys
COINGECKO_API_KEY=your-coingecko-api-key
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-api-key
PLAID_CLIENT_ID=your-plaid-client-id
PLAID_SECRET=your-plaid-secret
PLAID_ENVIRONMENT=sandbox

# Blockchain - Local/Test Networks
WEB3_PROVIDER_URL=http://localhost:8545
POLYGON_PROVIDER_URL=https://polygon-mumbai.infura.io/v3/your-infura-key
BSC_PROVIDER_URL=https://data-seed-prebsc-1-s1.binance.org:8545/

# AI Services - Development Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Payment Processing - Development
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
COINBASE_API_KEY=your-coinbase-api-key
COINBASE_API_SECRET=your-coinbase-api-secret

# Cloud Services - Development
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=finnexusai-dev-assets

# Monitoring - Development
SENTRY_DSN=your-sentry-dsn
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001

# Security - Development
ENCRYPTION_KEY=dev-encryption-key-32-chars-long
BCRYPT_ROUNDS=8

# Trading - Development (Test Mode)
BINANCE_API_KEY=your-binance-test-api-key
BINANCE_SECRET_KEY=your-binance-test-secret-key
COINBASE_PRO_API_KEY=your-coinbase-pro-test-api-key
COINBASE_PRO_SECRET_KEY=your-coinbase-pro-test-secret-key

# Compliance - Development
SHARIA_COMPLIANCE_ENABLED=true
GDPR_COMPLIANCE_ENABLED=true
PCI_DSS_COMPLIANCE_ENABLED=false
SOC2_COMPLIANCE_ENABLED=false

# Performance - Development
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000
CACHE_TTL_SECONDS=300
CDN_ENABLED=false

# HFT Configuration - Development (Disabled)
HFT_ENABLED=false
HFT_CO_LOCATION_ENABLED=false
HFT_MAX_LATENCY_MS=1000
HFT_RISK_LIMIT_USD=100

# Arbitrage Configuration - Development
ARBITRAGE_ENABLED=true
ARBITRAGE_MIN_PROFIT_PERCENTAGE=5.0
ARBITRAGE_MAX_POSITION_SIZE_USD=100

# Options & Derivatives - Development
DERIVATIVES_ENABLED=true
BLACK_SCHOLES_ENABLED=true
MONTE_CARLO_SIMULATIONS=1000

# Futurist Sandbox - Development
SANDBOX_ENABLED=true
QUANTUM_SIMULATOR_ENABLED=false
AI_MODELS_ENABLED=true

# Load Balancing - Development
LOAD_BALANCER_ENABLED=false
HEALTH_CHECK_INTERVAL=30000
HEALTH_CHECK_TIMEOUT=5000

# Backup Configuration - Development
BACKUP_ENABLED=false
BACKUP_INTERVAL_HOURS=24
BACKUP_RETENTION_DAYS=3

# Logging - Development
LOG_LEVEL=debug
LOG_FORMAT=pretty
LOG_FILE_MAX_SIZE=10MB
LOG_FILE_MAX_FILES=3

# SSL/TLS Configuration - Development
SSL_ENABLED=false
SSL_CERT_PATH=/path/to/dev-cert.crt
SSL_KEY_PATH=/path/to/dev-key.key

# CDN Configuration - Development
CDN_PROVIDER=none
CDN_CACHE_TTL=0
CDN_EDGE_CACHE_TTL=0

# Auto-scaling - Development
AUTO_SCALE_ENABLED=false
MIN_REPLICAS=1
MAX_REPLICAS=1
TARGET_CPU_UTILIZATION=80
TARGET_MEMORY_UTILIZATION=90

# Development Specific
HOT_RELOAD=true
DEBUG_MODE=true
MOCK_EXTERNAL_APIS=true
ENABLE_CORS=true

